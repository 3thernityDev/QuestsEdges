        "zod": "^4.1.13"
    },
    "devDependencies": {
        "@eslint/js": "^9.17.0",
        "@types/bcrypt": "^6.0.0",
        "@types/cors": "^2.8.19",
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/node": "^20.11.19",
        "bcrypt": "^6.0.0",
        "eslint": "^9.17.0",
        "eslint-config-prettier": "^10.0.1",
        "nodemon": "^3.1.11",
        "prettier": "^3.4.2",
